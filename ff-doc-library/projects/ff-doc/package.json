{
  "name": "@frankframework/ff-doc",
  "version": "1.2.0",
  "description": "A powerful and easy-to-use library designed to extract and process information from the FF! Doc JSON.",
  "main": "",
  "author": "Vivy Booman",
  "license": "Apache-2.0",
  "homepage": "https://frankframework.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/frankframework/frank-doc.git",
    "directory": "projects/ff-doc"
  },
  "optionalDependencies": {
    "@angular/core": "^20.2.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "module": "frankframework-ff-doc.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "default": "./frankframework-ff-doc.mjs"
    },
    "./angular": {
      "types": "./angular/index.d.ts",
      "default": "./angular/fesm2022/frankframework-ff-doc.mjs"
    },
    "./react": {
      "types": "./react/index.d.ts",
      "default": "./react/frankframework-ff-doc.mjs"
    }
  }
}
